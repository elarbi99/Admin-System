<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.adminsystem.UpdateItemController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <TextField fx:id="txtUpdatePrice" layoutX="259.0" layoutY="156.0" prefHeight="25.0" prefWidth="69.0" style="-fx-border-color: black; -fx-border-width: 2px;" />
            <Label layoutX="197.0" layoutY="161.0" prefHeight="17.0" prefWidth="49.0" text="Price:" />
            <TextField fx:id="txtUpdateOnHand" layoutX="259.0" layoutY="200.0" prefHeight="25.0" prefWidth="69.0" style="-fx-border-color: black; -fx-border-width: 2px;" />
            <Label layoutX="178.0" layoutY="205.0" prefHeight="17.0" prefWidth="69.0" text="On Hand:" />
            <Button layoutX="290.0" layoutY="275.0" mnemonicParsing="false" onAction="#update" prefHeight="25.0" prefWidth="54.0" style="-fx-background-color: #3BA1A5;" text="Add" />
            <Button layoutX="23.0" layoutY="351.0" mnemonicParsing="false" onAction="#back" style="-fx-background-color: #3BA1A5;" text="Back" />
            <Label layoutX="209.0" layoutY="125.0" prefHeight="17.0" prefWidth="38.0" text="ID:" />
            <TextField fx:id="txtItemID" layoutX="259.0" layoutY="120.0" prefHeight="25.0" prefWidth="116.0" style="-fx-border-color: black; -fx-border-width: 2px;" />
            <Label layoutX="211.0" layoutY="333.0" text="If you are not updating item put original field" />
            <Label layoutX="238.0" layoutY="40.0" prefHeight="24.0" prefWidth="130.0" text="Update Item">
               <font>
                  <Font size="19.0" />
               </font>
            </Label>
         </children>
    </AnchorPane>
  </children>
</VBox>
